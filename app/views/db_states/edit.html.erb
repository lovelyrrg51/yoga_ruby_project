<% content_for(:title, 'State') %>

<div class="adminCntrl">
    <div class="headingtittle-h2 text-center">
    <h2>Admin Panel</h2>
    </div>
    <div class="row">
        <div class="overflow-row">
            <%= render 'admin/admin_left_panel' %>
            <div class="rightCntrl">
                <div class="boxsection-lg boxsection-xs boxshadow prelative">
             
                    <div class="rightContent">
                        <div id="state_type">
                            <div class="row">
                                <div class="col-sm-12 mg-b30">
                                     <div class="headtittle text-center">
                                        <h2>States</h2>
                                        <span class="subheading"><%= @db_state.try(:country).try(:name) %> / <%= @db_state.try(:name) %></span>
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="pannel-btn text-left">
                                        <%= link_to "CREATE NEW", index_for_admin_db_country_db_states_path(@db_country), class: "btn btn-info" %>
                                    </div>
                                    <%= render "db_states/form" %>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>    
</div>
