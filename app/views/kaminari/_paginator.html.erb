<%= paginator.render do -%>
  <ul class="pagination hidden-xs">
    <%= prev_page_tag %>
    <% each_page do |page| -%>
      <% if page.left_outer? || page.right_outer? || page.inside_window? -%>
        <%= page_tag page %>
      <% end -%>
    <% end -%>
    <%= next_page_tag %>
  </ul>
  <ul class="pagination visible-xs">
      <%= prev_page_tag %>
      <%= page_tag current_page %>
      <%= next_page_tag %>
  </ul>
<% end -%>
