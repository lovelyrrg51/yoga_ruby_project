<% cols_count = 6 %>
<% cols_count += 3 if data.gateway.try(:dig, :payment_method) == 'Demand draft' %>
<% cols_count += (data.event_order.try(:tax_details).try(:dig, 'tax_breakup') || {}).length %>
<% cols_count += ((data.event_order.try(:tax_details) || {})['tax_breakup_on_convenience_charges'] || {}).length %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
    <title>Registration Receipt</title>
    <style>
    * {
        margin: 0;
        box-sizing: border-box;
    }

    body {
        font-family: Arial, Helvetica, sans-serif;
        font-size: 14px;
    }

    table th,
    table td {
        padding: 8px 12px;
        vertical-align: middle;
        text-align: left;
    }
    .label {
        display: inline;
        padding: .2em .6em .3em;
        font-size: 75%;
        font-weight: bold;
        line-height: 1;
        color: #fff;
        text-align: center;
        white-space: nowrap;
        vertical-align: baseline;
        border-radius: .25em;
    }
    .badge-custom {
        padding: 3px 10px;
        font-size: 11px;
        border-radius: 0;
    }
    .primary-bg {
        background-color: #990000;
    }
    </style>
</head>

<body>
    <table width="980px" style="margin: auto; border-spacing: 0; border-collapse: collapse;">
    	<tr>
        <td style="background: #990000; padding: 12px 15px;">
            <img src="https://s3.amazonaws.com/syportalattachments/email_template_images/shivyog-logo.png" width="140">
    		</td>
    		<!-- <td style="text-align: right; background: #990000; padding: 12px 15px;">
                <ul style="list-style: none;">
                    <li style="display: inline-block; padding: 0 5px 0 0;"><a href=""><img src='data:image/png;base64,'/></a></li>
                    <li style="display: inline-block; padding: 0 5px 0 0;"><a href=""><img src='data:image/png;base64,'/></a></li>
                    <li style="display: inline-block; padding: 0 5px 0 0;"><a href=""><img src='data:image/png;base64,'/></a></li>
                    <li style="display: inline-block; padding: 0 5px 0 0;"><a href=""><img src='data:image/png;base64,'/></a></li>
                </ul>
            </td> -->
    	</tr>
        <tr>
            <td colspan="2" style="text-align: center; padding: 8px 0;">
            	<div style="margin: 15px 0 0;">
	                <h2 style="font-size: 20px; margin: 0 0 5px;">Registration Summary</h2>
	                <span style="display: inline-block; font-size: 14px; text-transform: capitalize; background-color: #ffae82; padding: 5px 10px; color: #000000;"><%= data.event.try(:event_name_with_location) %></span>
	            </div>
            </td>
        </tr>
        <tr>
            <td colspan="2" style="padding: 8px 0;">
                <table width="100%" style="box-shadow: 0 0 5px rgba(0, 0, 0, 0.3); background: #ffffff; padding: 30px; margin: 20px 0;">
                    <tbody>
                        <tr>
                            <td width="50%">
                                <table>
                                    <tbody>
                                        <tr>
                                            <td style="vertical-align: top;">
                                                <% if data.event.try(:graced_by).try(:eql? , "Ishan ji") %>
                                                  <div style="background-image: url('data:image/png;base64,'); background-repeat: no-repeat; background-size: cover; width: 50px; height: 50px; border-radius: 50%; border: 4px solid #e0e0e0;"></div>
                                                <% elsif data.event.try(:graced_by).try(:eql? , "Baba ji") %>
                                                  <div style="background-image: url('data:image/png;base64,'); background-repeat: no-repeat; background-size: cover; width: 50px; height: 50px; border-radius: 50%; border: 4px solid #e0e0e0;"></div>
                                                <% else %>
                                                  <div style="background-image: url('data:image/png;base64,'); background-repeat: no-repeat; background-size: cover; width: 50px; height: 50px; border-radius: 50%; border: 4px solid #e0e0e0;"></div>
                                                <% end %>
                                            </td>
                                            <td style="vertical-align: top;">
                                                <h2 style="font-size: 16px; margin: 0 0 10px 10px;"><%= data.event.try(:graced_by) %></h2>
                                                <div style="margin: 0 0 10px 10px;">
                                                    <span style="text-transform: uppercase; font-weight: 700; color: #a4a4a4; display: block; margin: 0 0 5px;">Contact</span>
                                                    <p style="font-weight: normal; text-transform: capitalize; color: #000000;"><%= data.event.try(:contact_details) %></p>
                                                </div>
                                                <div style="margin: 0 0 0 10px;">
                                                    <span style="text-transform: uppercase; font-weight: 700; color: #a4a4a4; display: block; margin: 0 0 5px;">Venue</span>
                                                    <p style="font-weight: normal; text-transform: capitalize; color: #000000;"><%= data.event.try(:full_address) %></p>
                                                </div>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </td>
                            <td width="50%">
                                <div style="margin: 0 0 10px 0;">
                                    <span style="text-transform: uppercase; font-weight: 700; color: #a4a4a4; display: block; margin: 0 0 5px;">EVENT DATE</span>
                                    <p style="font-weight: normal; text-transform: capitalize; color: #000000;"><%= data.event.try(:event_start_date).try(:strftime, ('%B %d,%Y ')) %> To <%= data.event.try(:event_end_date).try(:strftime, ('%B %d,%Y ')) %></p>
                                </div>
                                <% if !data.event.try(:free?) && data.event.try(:event_discount_plan_associations).try(:present?) %>
                                    <div style="padding: 6px 10px; background-color: #ffae82;">
                                        <p style="font-size: 13px; font-weight: 700; text-transform: none; color: #000000;"><%= data.event.try(:discount_text).present? ? data.event.try(:discount_text) : "This event has special discounts for Fourm Members that will be visible when you reach the payment page" %></p>
                                    </div>
                                <% end %>
                            </td>
                        </tr>
                        <tr>
                            <td colspan="2" style="padding:20px 0 8px; font-size: 16px; font-weight: 700; margin: 10px 0 15px; border-top: 1px solid #e8e8e8;">Registration Details</td>
                        </tr>
                        <tr>
                            <td colspan="2" style="padding: 8px 0;">
                                <p style="font-weight: 700; margin: 15px 0; ">Registration Request Summary</p>
                                <table width="100%" style="border-collapse: collapse;">
                                    <thead style="background: #fff5ee; font-size: 12px; text-transform: uppercase; color: #636363; border-bottom: 2px solid #cccccc; border-top: 2px solid #cccccc;">
                                        <tr>
                                            <th style="padding: 12px 8px;">Event</th>
                                            <th style="padding: 12px 8px;">Registration Date</th>
                                            <th style="padding: 12px 8px;">Registration Ref No.</th>
                                            <th style="padding: 12px 8px;">Status</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td style="border-bottom: 1px solid #cccccc; line-height: 18px; color: #000000; padding: 12px 8px;"><%= data.event.try(:event_name_with_location) %></td>
                                            <td style="border-bottom: 1px solid #cccccc; line-height: 18px; color: #000000; padding: 12px 8px;"><%= data.event_order.try(:created_at).try(:strftime, ('%B %d,%Y ')) %></td>
                                            <td style="border-bottom: 1px solid #cccccc; line-height: 18px; color: #000000; padding: 12px 8px;"><%= data.event_order.try(:reg_ref_number) %></td>
                                            <td style="border-bottom: 1px solid #cccccc; line-height: 18px; color: #990000; padding: 12px 8px;"><%= data.event_order.try(:status).try(:titleize) %></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </td>
                        </tr>
                        <tr>
                            <td colspan="2" style="padding: 8px 0;">
                                <p style="font-weight: 700; margin: 15px 0; ">Payment Details</p>
                                <table width="100%" style="border-collapse: collapse;">
                                    <thead style="background: #fff5ee; font-size: 12px; text-transform: uppercase; color: #636363;">
                                        <tr>
                                            <% if data.gateway.try(:dig, :payment_method) == 'Demand draft' %>
                                              <th style="padding: 12px 8px; border: 1px solid #cccccc;">Demand Draft Number</th>
                                              <th style="padding: 12px 8px; border: 1px solid #cccccc;">Demand Draft Date</th>
                                              <th style="padding: 12px 8px; border: 1px solid #cccccc;">Bank Name</th>
                                            <% end %>
                                            <th style="padding: 12px 8px; border: 1px solid #cccccc;">Payment Date</th>
                                            <th style="padding: 12px 8px; border: 1px solid #cccccc;">Registration Fees</th>
                                            <th style="padding: 12px 8px; border: 1px solid #cccccc;"><%= (data.event_order.try(:tax_details) || {})['total_discount'].rnd.negative? ? 'Payable Discount' : 'Discount' %></th>
                                            <th style="padding: 12px 8px; border: 1px solid #cccccc;">Amount</th>
                                            <% (data.event_order.try(:tax_details).try(:dig, 'tax_breakup') || {}).each do |tax| %>
                                              <th style="padding: 12px 8px; border: 1px solid #cccccc;"><%= tax['tax_name'] %></th>
                                            <% end %>
                                            <th style="padding: 12px 8px; border: 1px solid #cccccc;">Total Tax (<%= (data.event_order.try(:tax_details) || {})['total_tax_percentage'] %>%)</th>
                                            <th style="padding: 12px 8px; border: 1px solid #cccccc;">Transaction Charges</th>
                                            <% ((data.event_order.try(:tax_details) || {})['tax_breakup_on_convenience_charges'] || {}).each do |tax| %>
                                              <th style="padding: 12px 8px; border: 1px solid #cccccc;"><%= tax['tax_name'] %> (<%= tax['percent'] %>%)</th>
                                            <% end %>         
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <% if data.gateway.try(:dig, :payment_method) == 'Demand draft' %>
                                              <td style="border: 1px solid #cccccc; border-top: 0; color: #000000; padding: 12px 8px;"><%= data.payment.try(:dd_number) %></td>
                                              <td style="border: 1px solid #cccccc; border-top: 0; color: #990000; padding: 12px 8px;"><%= data.payment.try(:dd_date) %></td>
                                              <td style="border: 1px solid #cccccc; border-top: 0; color: #000000; padding: 12px 8px;"><%= data.payment.try(:bank_name) %></td>
                                            <% end %>
                                            <td style="border: 1px solid #cccccc; border-top: 0; color: #000000; padding: 12px 8px;"><%= data.payment.try(:created_at).try(:strftime, ('%B %d, %Y ')) %></td>
                                            <td style="border: 1px solid #cccccc; border-top: 0; color: #000000; padding: 12px 8px;"><%= "#{data.currency} #{number_with_precision((data.event_order.try(:tax_details).try(:dig, 'original_amount').rnd + data.event_order.try(:tax_details).try(:dig, 'total_discount').rnd) , precision: 2)}" %></td>
                                            <td style="border: 1px solid #cccccc; border-top: 0; color: #000000; padding: 12px 8px;"><%= "#{data.currency} #{number_with_precision(data.event_order.try(:tax_details).try(:dig, 'total_discount').rnd.abs, precision: 2)}" %></td>
                                            <td style="border: 1px solid #cccccc; border-top: 0; color: #000000; padding: 12px 8px;"><%= "#{data.currency} #{number_with_precision(data.event_order.try(:tax_details).try(:dig, 'original_amount').rnd.abs, precision: 2)}" %></td>
                                            <% (data.event_order.try(:tax_details).try(:dig, 'tax_breakup') || {}).each do |tax| %> 
                                              <td style="border: 1px solid #cccccc; border-top: 0; color: #000000; padding: 12px 8px;"><%= "#{data.currency} #{number_with_precision(tax['amount'].rnd, precision: 2)}" %></td>
                                            <% end %>
                                            
                                            <td style="border: 1px solid #cccccc; border-top: 0; color: #000000; padding: 12px 8px; font-weight: 700; font-size: 13px;"><%= "#{data.currency} #{number_with_precision((data.event_order.try(:tax_details) || {})['total_tax_applied'].rnd, precision: 2)}" %></td>
                                            <td style="border: 1px solid #cccccc; border-top: 0; color: #000000; padding: 12px 8px; font-weight: 700; font-size: 13px;"><%= "#{data.currency} #{number_with_precision(((data.event_order.try(:tax_details) || {})['transaction_charges'] || (data.event_order.try(:tax_details) || {})['convenience_charges']).rnd, precision: 2)}" %></td>
                                            <% ((data.event_order.try(:tax_details) || {})['tax_breakup_on_convenience_charges'] || {}).each do |tax| %>
                                              <td style="border: 1px solid #cccccc; border-top: 0; color: #000000; padding: 12px 8px; font-weight: 700; font-size: 13px;"><%= "#{data.currency} #{number_with_precision(tax['amount'].rnd, precision: 2)}" %></td>
                                            <% end %>                                            
                                        </tr>
                                        <tr>
                                        	<td colspan="<%= cols_count - 1 %>" style="font-size: 13px; font-weight: 700; border: 1px solid #cccccc; border-top: 0; color: #000000; padding: 12px 8px;">Net Amount <% if data.event.try(:country_id) == 113 && !data.event.try(:free?) %> <span class="label label-danger primary-bg badge-custom">Non Refundable</span> <% end %> </td>
                                        	<td style="font-size: 13px; font-weight: 700; border: 1px solid #cccccc; border-top: 0; color: #000000; padding: 12px 8px;"><%= "#{data.currency} #{number_with_precision((data.event_order.try(:tax_details) || {})['total_payable_amount'].rnd, precision: 2)}" %></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </td>
                        </tr>
                        <tr>
                            <td colspan="2" style="padding: 8px 0;">
                                <p style="font-weight: 700; margin: 15px 0; ">Registration Summary</p>
                                <table width="100%" style="border-collapse: collapse;">
                                    <thead style="background: #fff5ee; font-size: 12px; text-transform: uppercase; color: #636363; border-bottom: 2px solid #cccccc; border-top: 2px solid #cccccc;">
                                        <tr>
                                            <th style="padding: 12px 8px;">S.No.</th>
                                            <th style="padding: 12px 8px;">REGISTRATION NUMBER</th>
                                            <th style="padding: 12px 8px;">Name / SYID</th>
                                            <th style="padding: 12px 8px;">SEATING CATEGORY</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                      <% (data.event_order_line_items || []).each_with_index do |item, item_index| %>
                                        <tr>
                                            <td style="border-bottom: 1px solid #cccccc; line-height: 18px; color: #000000;"><%= item_index + 1 %>.</td>
                                            <td style="border-bottom: 1px solid #cccccc; line-height: 18px; color: #000000;"><%= item.event_registration.try(:serial_number).to_i + 100%></td>
                                            <td style="border-bottom: 1px solid #cccccc; line-height: 18px; color: #000000;">
                                                <div>
                                                    <p style="color: #000000; font-weight:700; font-size: 13px;"><%= item.full_name %></p>
                                                    <span style="color: #990000; font-weight:700; font-size: 12px;"><%= item.syid %></span>
                                                </div>
                                            </td>
                                            <td style="border-bottom: 1px solid #cccccc; line-height: 18px; color: #990000;">
                                                <div>
                                                    <p style="color: #000000; font-weight:700; font-size: 13px;"><%= item.category_name %></p>
                                                    <span style="color: #990000; font-weight:700; font-size: 12px;"><%= "#{data.currency} #{item.category_price.rnd}" %></span>
                                                </div>
                                            </td>
                                        </tr>
                                      <% end %>
                                    </tbody>
                                </table>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </td>
        </tr>
        <tr>
            <td colspan="2" style="padding: 12px 15px; background: #4a4545; text-align: center;">
                <div>
                    <p style="color: #ffffff; margin: 0; font-size: 14px;">If you need any help please mail us at <a href="" style="color: #d2691e; margin: 0 0 0 5px; text-decoration: underline; cursor: pointer;">registratio@shivyogindia.com</a></p>
                    <p style="color: #ffffff; margin: 10px 0 0; font-size: 13px;">&copy; ShivYog. All Rights Reserved.</p>
                </div>
            </td>
        </tr>
    </table>
</body>

</html>